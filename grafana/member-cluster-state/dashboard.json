  "panels": [
    {
      "type": "row",
      "title": "Overview",
      "id": 1,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "stat",
      "title": "Member cluster state",
      "id": 2,
      "datasource": "$DS_PROMETHEUS",
      "targets": [
        {
          "refId": "A",
          "expr": "max by (cluster_name) (cluster_ready_state{cluster_name=~\"$cluster\"})",
          "legendFormat": "{{cluster_name}}"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "mappings": [
            {
              "type": "value",
              "options": {
                "1": {
                  "text": "\u2705 Ready"
                },
                "0": {
                  "text": "\u274c Not ready"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red"
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "links": [
            {
              "title": "Open cluster details",
              "url": "/d/karmada-clusters?var=cluster=${__series.name}",
              "targetBlank": false
            }
          ]
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Member clusters",
      "id": 4,
      "datasource": "$DS_PROMETHEUS",
      "targets": [
        {
          "refId": "A",
          "expr": "count(count by (cluster_name) (cluster_ready_state))"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 1
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Total nodes by cluster",
      "id": 5,
      "datasource": "$DS_PROMETHEUS",
      "targets": [
        {
          "refId": "A",
          "expr": "max by (cluster_name) (cluster_node_number{cluster_name=~\"$cluster\"}) and on (cluster_name) max by (cluster_name) (cluster_ready_state == 1)",
          "legendFormat": "{{cluster_name}}"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 7
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "type": "bargauge",
      "title": "Ready nodes % by cluster",
      "id": 3,
      "datasource": "$DS_PROMETHEUS",
      "targets": [
        {
          "refId": "A",
          "expr": "100 * max by (cluster_name) (clamp_min(cluster_ready_node_number{cluster_name=~\"$cluster\"},0) / clamp_min(cluster_node_number{cluster_name=~\"$cluster\"},1)) and on (cluster_name) max by (cluster_name) (cluster_ready_state == 1)",
          "legendFormat": "{{cluster_name}}"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 7
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "basic"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red"
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "green",
                "value": 90
              }
            ]
          }
        },
        "overrides": []
      }
    },
