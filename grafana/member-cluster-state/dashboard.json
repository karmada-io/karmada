  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROMETHEUS",
        "label": "Prometheus",
        "query": "prometheus",
        "hide": 0
      },
      {
        "name": "cluster",
        "label": "Cluster",
        "type": "query",
        "datasource": "$DS_PROMETHEUS",
        "query": "label_values(cluster_ready_state, cluster_name)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        }
      }
    ]
